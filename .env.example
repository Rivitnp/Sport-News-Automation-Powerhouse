# Copy this file to .env and fill in your values
# DO NOT commit .env to git!

# Required
SERPER_KEY_MAIN=your_serper_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here
WP_URL=https://yoursite.com
WP_USERNAME=your_wordpress_username
WP_APP_PASSWORD=your_wordpress_app_password

# Optional
SERPER_KEY_BACKUP=backup_serper_key_for_rotation

# Image Generation - APIFree.ai (Primary, recommended)
# Fast, cheap ($0.004/image), high quality SDXL-based
# Get API key: https://apifree.ai/api-keys
APIFREE_API_KEY=your_apifree_api_key_here

# Image Generation - Cloudflare (Fallback, optional)
# Free, good quality, automatic fallback if APIFree fails
# Get credentials: https://dash.cloudflare.com/
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_TOKEN=your_cloudflare_api_token

# Model Configuration
OPENROUTER_MODEL=deepseek/deepseek-chat
DB_PATH=news_cache.db

# Analytics & SEO
GA_MEASUREMENT_ID=G-XXXXXXXXXX
# Get from: https://analytics.google.com/
